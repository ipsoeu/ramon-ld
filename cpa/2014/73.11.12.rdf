    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">73.11.12</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">73.11.12</skos:notation>
    <skos:prefLabel xml:lang="en">Direct marketing and direct mailing services</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - the developing and organisation of direct marketing advertising campaigns, that is, organising sending of advertising and promotional messages directly to consumers, rather than via mass media Includes methods such as direct mail and telemarketing.</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - the developing and organisation of direct marketing advertising campaigns, that is, organising sending of advertising and promotional messages directly to consumers, rather than via mass media Includes methods such as direct mail and telemarketing.</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - mailing services, see 82.19.12</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/cpa/2014/73.11.1"/>
