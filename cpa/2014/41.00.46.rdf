  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="http://localhost/ramon-ld/cpa/2014/41.00.46">
    <skos:inScheme rdf:resource="http://localhost/ramon-ld/cpa/2014"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.46</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.46</skos:notation>
    <skos:prefLabel xml:lang="en">Construction works in reservoirs, silos and warehouses</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - construction works for reservoirs and tanks - construction works for reservoirs for oil and gas - construction works for silos for cereals, cement or other dry aggregates - construction works for cold stores and specialised warehouses</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - construction works for reservoirs and tanks - construction works for reservoirs for oil and gas - construction works for silos for cereals, cement or other dry aggregates - construction works for cold stores and specialised warehouses</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="all">This subcategory also includes: - construction works for storage areas</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - construction works for agricultural silos and storage buildings used for agriculture, see 41.00.42 - construction works for water towers, see 42.21.22 - construction works for hydrocarbon terminals, see 42.99.21</xkos:exclusionNote>
    <skos:broader rdf:resource="http://localhost/ramon-ld/cpa/2014/41.00.4"/>
  </rdf:Description>
