    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">64.92.13</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">64.92.13</skos:notation>
    <skos:prefLabel xml:lang="en">Residential mortgage credit granting services, other than by monetary institutions</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - services of granting loans other than by monetary institutions for the purpose of acquiring residential land or buildings and for which the land or buildings are used as security - home equity loans</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - services of granting loans other than by monetary institutions for the purpose of acquiring residential land or buildings and for which the land or buildings are used as security - home equity loans</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - appraisal services, see 68.31.16</xkos:exclusionNote>
