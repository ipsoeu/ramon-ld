  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://ipsoeu.github.io/ramon-ld/cpa/2014/41.00.14">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/cpa/2014"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.14</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.14</skos:notation>
    <skos:prefLabel xml:lang="en">Residences for communities</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - residential buildings for communities, including residences and service residences for the elderly, students, children and other social groups, e.g. retirement homes, workers' hostels, fraternity homes, orphanages, hostels for the homeless etc.</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - residential buildings for communities, including residences and service residences for the elderly, students, children and other social groups, e.g. retirement homes, workers' hostels, fraternity homes, orphanages, hostels for the homeless etc.</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - hospitals and clinics, see 41.00.28 - institutional buildings with nursing or medical care, see 41.00.28 - prisons, barracks, see 41.00.29</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/cpa/2014/41.00.1"/>
