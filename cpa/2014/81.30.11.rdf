    <skos:inScheme rdf:resource="https://w3id.org/ipsoeu/ramon-ld/cpa/2014"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">81.30.11</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">81.30.11</skos:notation>
    <skos:prefLabel xml:lang="en">Home garden maintenance</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - planting, care and maintenance services of parks and gardens for households</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - planting, care and maintenance services of parks and gardens for households</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="all">This subcategory also includes: - minor incidental design and construction services (ground modelling, erection of retaining walls, walkways etc.)</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - commercial production and planting services for commercial production of plants, trees, see divisions 01, 02 - tree nursery and forest tree nursery services, see 01.30.10, 02.10.20 - construction works for landscaping purposes, see section F - landscape design and architecture services, see 71.11.4</xkos:exclusionNote>
    <skos:broader rdf:resource="https://w3id.org/ipsoeu/ramon-ld/cpa/2014/81.30.1"/>
  </rdf:Description>
