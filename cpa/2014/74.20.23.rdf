  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://w3id.org/ipsoeu/ramon-ld/cpa/2014/74.20.23">
    <skos:inScheme rdf:resource="https://w3id.org/ipsoeu/ramon-ld/cpa/2014"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">74.20.23</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">74.20.23</skos:notation>
    <skos:prefLabel xml:lang="en">Event photography and event videography services</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - services consisting of photographing or videotaping live events, such as weddings, graduations, conventions, receptions, fashion shows, sports and news events, and any other events of current interest</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - services consisting of photographing or videotaping live events, such as weddings, graduations, conventions, receptions, fashion shows, sports and news events, and any other events of current interest</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - motion picture, video and television programme production services, see 59.11.1 - news agency services, see 63.91.1</xkos:exclusionNote>
    <skos:broader rdf:resource="https://w3id.org/ipsoeu/ramon-ld/cpa/2014/74.20.2"/>
  </rdf:Description>
