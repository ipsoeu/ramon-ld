  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="http://localhost/ramon-ld/cpa/2014/41.00.41">
    <skos:inScheme rdf:resource="http://localhost/ramon-ld/cpa/2014"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.41</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.41</skos:notation>
    <skos:prefLabel xml:lang="en">Construction works in industrial buildings</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - construction works for covered buildings used for industrial production, e.g. factories, workshops, slaughterhouses, breweries, assembly plants etc.</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - construction works for covered buildings used for industrial production, e.g. factories, workshops, slaughterhouses, breweries, assembly plants etc.</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - construction works for reservoirs, silos and warehouses, see 41.00.46 - construction works for non-residential farm buildings, see 41.00.42 - construction works for complex industrial installations (power stations, refineries etc.) which do not have the characteristics of a building, see 42.22.23, 42.99.21</xkos:exclusionNote>
    <skos:broader rdf:resource="http://localhost/ramon-ld/cpa/2014/41.00.4"/>
  </rdf:Description>
