  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://w3id.org/ipsoeu/ramon-ld/cpa/2014/84.30.14">
    <skos:inScheme rdf:resource="https://w3id.org/ipsoeu/ramon-ld/cpa/2014"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">84.30.14</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">84.30.14</skos:notation>
    <skos:prefLabel xml:lang="en">Compulsory social security services concerning family and child allowances</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - public administrative, operational and support services related to income assistance for households and families with dependent children - payments to families with dependent children whether or not an income recipient is part of the household - payment to households on a per-child basis, regardless of needs</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - public administrative, operational and support services related to income assistance for households and families with dependent children - payments to families with dependent children whether or not an income recipient is part of the household - payment to households on a per-child basis, regardless of needs</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - maternity benefit services, see 84.30.11</xkos:exclusionNote>
    <skos:broader rdf:resource="https://w3id.org/ipsoeu/ramon-ld/cpa/2014/84.30.1"/>
  </rdf:Description>
</rdf:RDF>
