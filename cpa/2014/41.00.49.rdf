<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcat="http://www.w3.org/ns/dcat#" xmlns:gsp="http://www.opengis.net/ont/geosparql#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:xkos="http://rdf-vocabulary.ddialliance.org/xkos#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:wdrs="http://www.w3.org/2007/05/powder-s#">
  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://ipsoeu.github.io/ramon-ld/cpa/2014/41.00.49">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/cpa/2014"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.49</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.49</skos:notation>
    <skos:prefLabel xml:lang="en">Construction works on other non-residential buildings</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - construction works for penitentiaries, prisons and remand centres, barracks for armed forces, police or fire services</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - construction works for penitentiaries, prisons and remand centres, barracks for armed forces, police or fire services</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="all">This subcategory also includes: - construction works for statues and commemorative, artistic or decorative constructions - construction works for structures such as bus shelters, public toilets, wash houses etc.</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - construction works for telephone booths, see 41.00.45 - construction works for museums, see 41.00.48 - construction works for religious buildings, see 41.00.48 - construction works for hospitals of penitentiaries, prisons and armed forces, see 41.00.48 - construction works for military engineering works, see 42.99.29</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/cpa/2014/41.00.4"/>
  </rdf:Description>
</rdf:RDF>
