  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://ipsoeu.github.io/ramon-ld/cpa/2014/41.00.31">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/cpa/2014"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.31</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">41.00.31</skos:notation>
    <skos:prefLabel xml:lang="en">Construction works in one-dwelling buildings</skos:prefLabel>
    <skos:definition xml:lang="all">This subcategory includes: - construction works for detached houses such as bungalows, villas, chalets, forest lodges, farmhouses, country houses, summer houses, weekend houses etc.</skos:definition>
    <xkos:coreContentNote xml:lang="all">This subcategory includes: - construction works for detached houses such as bungalows, villas, chalets, forest lodges, farmhouses, country houses, summer houses, weekend houses etc.</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="all">This subcategory also includes: - construction works for semi-detached or terraced houses, with each dwelling having its own roof and its own entrance directly from ground surface</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="all">This subcategory excludes: - construction works for non-residential farm buildings, see 41.00.42</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/cpa/2014/41.00.3"/>
  </rdf:Description>
