  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://w3id.org/ipsoeu/ramon-ld/nace/1990/75.23">
    <skos:inScheme rdf:resource="https://w3id.org/ipsoeu/ramon-ld/nace/1990"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">75.23</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">75.23</skos:notation>
    <skos:prefLabel xml:lang="en">Justice and judicial activities</skos:prefLabel>
    <skos:definition xml:lang="en">- administration and operation of administrative civil and criminal law courts, military tribunals and the judicial system - prison administration and provision of correctional services</skos:definition>
    <xkos:coreContentNote xml:lang="en">- administration and operation of administrative civil and criminal law courts, military tribunals and the judicial system - prison administration and provision of correctional services</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="en">- rehabilitation services</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="en">- advice and representation in civil, criminal and other cases, see 74.11 - activities of prison schools, see 80 - activities of prison hospitals, see 85.11</xkos:exclusionNote>
    <skos:broader rdf:resource="https://w3id.org/ipsoeu/ramon-ld/nace/1990/75.2"/>
  </rdf:Description>
