  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="http://localhost/ramon-ld/nace/2002/80.42">
    <skos:inScheme rdf:resource="http://localhost/ramon-ld/nace/2002"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">80.42</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">80.42</skos:notation>
    <skos:prefLabel xml:lang="en">Adult and other education n.e.c.</skos:prefLabel>
    <skos:definition xml:lang="en">This class includes: &#x2013; adult education, i.e. education for people who are not in the regular school and university system. Instruction may be given in day or evening classes in schools or in special institutions providing for adults &#x2013; education which is not definable by level</skos:definition>
    <xkos:coreContentNote xml:lang="en">This class includes: &#x2013; adult education, i.e. education for people who are not in the regular school and university system. Instruction may be given in day or evening classes in schools or in special institutions providing for adults &#x2013; education which is not definable by level</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="en">This class excludes: &#x2013; higher education, see 80.30 &#x2013; activities of dance schools, see 92.34 &#x2013; instruction in sport and games, see 92.62</xkos:exclusionNote>
    <skos:broader rdf:resource="http://localhost/ramon-ld/nace/2002/80.4"/>
  </rdf:Description>
