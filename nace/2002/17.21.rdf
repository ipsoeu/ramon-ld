  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://ipsoeu.github.io/ramon-ld/nace/2002/17.21">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2002"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">17.21</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">17.21</skos:notation>
    <skos:prefLabel xml:lang="en">Cotton-type weaving</skos:prefLabel>
    <skos:definition xml:lang="en">This class includes: &#x2013; manufacture of broad woven cotton-type fabrics, either with cotton or artificial or synthetic yarns</skos:definition>
    <xkos:coreContentNote xml:lang="en">This class includes: &#x2013; manufacture of broad woven cotton-type fabrics, either with cotton or artificial or synthetic yarns</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="en">This class also includes: &#x2013; manufacture of woven pile or chenille fabrics, terry towelling, gauze, etc.</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="en">This class excludes: &#x2013; manufacture of textile floor coverings, see 17.51 &#x2013; manufacture of non-wovens, see 17.53 &#x2013; manufacture of narrow fabrics, see 17.54 &#x2013; manufacture of knitted and crocheted fabrics, see 17.60</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2002/17.2"/>
  </rdf:Description>
