  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://ipsoeu.github.io/ramon-ld/nace/2008/51.10">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2008"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">51.10</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">51.10</skos:notation>
    <skos:prefLabel xml:lang="en">Passenger air transport</skos:prefLabel>
    <skos:definition xml:lang="all">This class includes: - transport of passengers by air over regular routes and on regular schedules - charter flights for passengers - scenic and sightseeing flights</skos:definition>
    <xkos:coreContentNote xml:lang="all">This class includes: - transport of passengers by air over regular routes and on regular schedules - charter flights for passengers - scenic and sightseeing flights</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="all">This class also includes: - rental of air transport equipment with operator for the purpose of passenger transportation - general aviation activities, such as: &#x2022; transport of passengers by aero clubs for instruction or pleasure</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="all">This class excludes: - rental of air transport equipment without operator, see 77.35</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2008/51.1"/>
  </rdf:Description>
