  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="http://localhost/ramon-ld/nace/2008/82">
    <skos:inScheme rdf:resource="http://localhost/ramon-ld/nace/2008"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">82</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">82</skos:notation>
    <skos:prefLabel xml:lang="en">Office administrative, office support and other business support activities</skos:prefLabel>
    <skos:definition xml:lang="all">This division includes the provision of a range of day-to-day office administrative services, as well as ongoing routine business support functions for others, on a contract or fee basis.</skos:definition>
    <xkos:coreContentNote xml:lang="all">This division includes the provision of a range of day-to-day office administrative services, as well as ongoing routine business support functions for others, on a contract or fee basis.</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="all">This division also includes all support service activities typically provided to businesses not elsewhere classified.</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="all">Units classified in this division do not provide operating staff to carry out the complete operations of a business.</xkos:exclusionNote>
