    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2008"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">26.80</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">26.80</skos:notation>
    <skos:prefLabel xml:lang="en">Manufacture of magnetic and optical media</skos:prefLabel>
    <skos:definition xml:lang="all">This class includes the manufacture of magnetic and optical recording media. This class includes: - manufacture of blank magnetic audio and video tapes - manufacture of blank magnetic audio and video cassettes - manufacture of blank diskettes - manufacture of blank optical discs - manufacture of hard drive media</skos:definition>
    <xkos:coreContentNote xml:lang="all">This class includes the manufacture of magnetic and optical recording media. This class includes: - manufacture of blank magnetic audio and video tapes - manufacture of blank magnetic audio and video cassettes - manufacture of blank diskettes - manufacture of blank optical discs - manufacture of hard drive media</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="all">This class excludes: - reproduction of recorded media (computer media, sound, video, etc.), see 18.2</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2008/26.8"/>
