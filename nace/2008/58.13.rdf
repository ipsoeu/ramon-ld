  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://ipsoeu.github.io/ramon-ld/nace/2008/58.13">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2008"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">58.13</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">58.13</skos:notation>
    <skos:prefLabel xml:lang="en">Publishing of newspapers</skos:prefLabel>
    <skos:definition xml:lang="all">This class includes the publishing of newspapers, including advertising newspapers, appearing at least four times a week. Publishing can be done in print or electronic form, including on the Internet.</skos:definition>
    <xkos:coreContentNote xml:lang="all">This class includes the publishing of newspapers, including advertising newspapers, appearing at least four times a week. Publishing can be done in print or electronic form, including on the Internet.</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="all">This class excludes: - news agency activities, see 63.91</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2008/58.1"/>
  </rdf:Description>
