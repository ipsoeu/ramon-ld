  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="http://localhost/ramon-ld/nace/2008/03.12">
    <skos:inScheme rdf:resource="http://localhost/ramon-ld/nace/2008"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">03.12</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">03.12</skos:notation>
    <skos:prefLabel xml:lang="en">Freshwater fishing</skos:prefLabel>
    <skos:definition xml:lang="all">This class includes: - fishing on a commercial basis in inland waters - taking of freshwater crustaceans and molluscs - taking of freshwater aquatic animals</skos:definition>
    <xkos:coreContentNote xml:lang="all">This class includes: - fishing on a commercial basis in inland waters - taking of freshwater crustaceans and molluscs - taking of freshwater aquatic animals</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="all">This class also includes: - gathering of freshwater materials</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="all">This class excludes: - processing of fish, crustaceans and molluscs, see 10.20 - fishing inspection, protection and patrol services, see 84.24 - fishing practiced for sport or recreation and related services, see 93.19 - operation of sport fishing preserves, see 93.19</xkos:exclusionNote>
    <skos:broader rdf:resource="http://localhost/ramon-ld/nace/2008/03.1"/>
  </rdf:Description>
