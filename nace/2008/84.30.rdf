  <rdf:Description xmlns:dct="http://purl.org/dc/terms/" rdf:about="https://ipsoeu.github.io/ramon-ld/nace/2008/84.30">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2008"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">84.30</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">84.30</skos:notation>
    <skos:prefLabel xml:lang="en">Compulsory social security activities</skos:prefLabel>
    <skos:definition xml:lang="all">This class includes: - funding and administration of government-provided social security programmes: &#x2022; sickness, work-accident and unemployment insurance &#x2022; retirement pensions &#x2022; programmes covering losses of income due to maternity, temporary disablement, widowhood etc.</skos:definition>
    <xkos:coreContentNote xml:lang="all">This class includes: - funding and administration of government-provided social security programmes: &#x2022; sickness, work-accident and unemployment insurance &#x2022; retirement pensions &#x2022; programmes covering losses of income due to maternity, temporary disablement, widowhood etc.</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="all">This class excludes: - non-compulsory social security, see 65.30 - provision of welfare services and social work (without accommodation), see 88.10, 88.99</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2008/84.3"/>
  </rdf:Description>
