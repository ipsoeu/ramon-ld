    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">11.02</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">11.02</skos:notation>
    <skos:prefLabel xml:lang="en">Manufacture of wine from grape</skos:prefLabel>
    <skos:definition xml:lang="all">This class includes: - manufacture of wine - manufacture of sparkling wine - manufacture of wine from concentrated grape must</skos:definition>
    <xkos:coreContentNote xml:lang="all">This class includes: - manufacture of wine - manufacture of sparkling wine - manufacture of wine from concentrated grape must</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="all">This class also includes: - blending, purification and bottling of wine - manufacture of low or non-alcoholic wine</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="all">This class excludes: - merely bottling and labelling, see 46.34 (if performed as part of wholesale) and 82.92 (if performed on a fee or contract basis)</xkos:exclusionNote>
