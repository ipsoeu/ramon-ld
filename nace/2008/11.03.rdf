    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/2008"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">11.03</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">11.03</skos:notation>
    <skos:prefLabel xml:lang="en">Manufacture of cider and other fruit wines</skos:prefLabel>
    <skos:definition xml:lang="all">This class includes: - manufacture of fermented but not distilled alcoholic beverages: sake, cider, perry and other fruit wines</skos:definition>
    <xkos:coreContentNote xml:lang="all">This class includes: - manufacture of fermented but not distilled alcoholic beverages: sake, cider, perry and other fruit wines</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="all">This class also includes: - manufacture of mead and mixed beverages containing fruit wines</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="all">This class excludes: - merely bottling and labelling, see 46.34 (if performed as part of wholesale) and 82.92 (if performed on a fee or contract basis)</xkos:exclusionNote>
