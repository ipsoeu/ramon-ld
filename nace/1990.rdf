    <skos:prefLabel xml:lang="en">Activities of travel agencies and tour operators; tourist assistance activities n.e.c.</skos:prefLabel>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/63"/>
    <skos:narrower rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/63.30"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://ipsoeu.github.io/ramon-ld/nace/1990/63.30">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
