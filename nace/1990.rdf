    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">01.30</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">01.30</skos:notation>
    <skos:prefLabel xml:lang="en">Growing of crops combined with farming of animals (mixed farming)</skos:prefLabel>
    <skos:definition xml:lang="en">- crop growing in combination with farming of livestock at mixed activity units with a specialization ratio in either one of less than 66% of standard gross margins</skos:definition>
    <xkos:coreContentNote xml:lang="en">- crop growing in combination with farming of livestock at mixed activity units with a specialization ratio in either one of less than 66% of standard gross margins</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="en">- mixed cropping or mixed livestock units, see their main activity</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/01.3"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://ipsoeu.github.io/ramon-ld/nace/1990/01.4">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
