  <rdf:Description rdf:about="https://ipsoeu.github.io/ramon-ld/nace/1990/75.1">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">75.1</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">75.1</skos:notation>
    <skos:prefLabel xml:lang="en">Administration of the State and the economic and social policy of the community</skos:prefLabel>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/75"/>
    <skos:narrower rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/75.11"/>
