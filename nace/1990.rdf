    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">29.1</skos:notation>
    <skos:prefLabel xml:lang="en">Manufacture of machinery for the production and use of mechanical power, except aircraft, vehicle and cycle engines</skos:prefLabel>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/29"/>
    <skos:narrower rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/29.11"/>
