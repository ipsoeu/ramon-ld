    <skos:prefLabel xml:lang="en">Manufacture of dairy products</skos:prefLabel>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/15"/>
    <skos:narrower rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/15.51"/>
