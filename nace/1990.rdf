  </rdf:Description>
  <rdf:Description rdf:about="https://ipsoeu.github.io/ramon-ld/nace/1990/17.12">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">17.12</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">17.12</skos:notation>
    <skos:prefLabel xml:lang="en">Preparation and spinning of woollen-type fibres</skos:prefLabel>
    <skos:definition xml:lang="en">- preparatory operations on woollen-type fibres: degreasing and carbonizing of wool, carding - manufacture of woollen-type yarns, either with wool or artificial or synthetic fibres, for weaving, knitting, etc.</skos:definition>
    <xkos:coreContentNote xml:lang="en">- preparatory operations on woollen-type fibres: degreasing and carbonizing of wool, carding - manufacture of woollen-type yarns, either with wool or artificial or synthetic fibres, for weaving, knitting, etc.</xkos:coreContentNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/17.1"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://ipsoeu.github.io/ramon-ld/nace/1990/17.13">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
