    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">63.4</skos:notation>
    <skos:prefLabel xml:lang="en">Activities of other transport agencies</skos:prefLabel>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/63"/>
