    <skos:prefLabel xml:lang="en">Manufacture of other general purpose machinery</skos:prefLabel>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/29"/>
    <skos:narrower rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/29.21"/>
