    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">15.62</skos:notation>
    <skos:prefLabel xml:lang="en">Manufacture of starches and starch products</skos:prefLabel>
    <skos:definition xml:lang="en">- manufacture of starches from rice, potatoes, maize, etc. - wet corn milling - manufacture of glucose, glucose syrup, maltose, etc. - manufacture of gluten - manufacture of tapioca - manufacture of corn oil</skos:definition>
    <xkos:coreContentNote xml:lang="en">- manufacture of starches from rice, potatoes, maize, etc. - wet corn milling - manufacture of glucose, glucose syrup, maltose, etc. - manufacture of gluten - manufacture of tapioca - manufacture of corn oil</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="en">- manufacture of lactose, see 15.51 - production of cane or beet sugar, see 15.83</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/15.6"/>
