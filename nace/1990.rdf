    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">37.2</skos:notation>
    <skos:prefLabel xml:lang="en">Recycling of non-metal waste and scrap</skos:prefLabel>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/37"/>
    <skos:narrower rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/37.20"/>
