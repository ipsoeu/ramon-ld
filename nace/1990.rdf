    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">18.22</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">18.22</skos:notation>
    <skos:prefLabel xml:lang="en">Manufacture of other outerwear</skos:prefLabel>
    <skos:definition xml:lang="en">- manufacture of other outerwear made of woven, knitted or crocheted fabric, non-wovens, etc. for men, women and children: • coats, suits, ensembles, jackets, trousers, skirts, etc.</skos:definition>
    <xkos:coreContentNote xml:lang="en">- manufacture of other outerwear made of woven, knitted or crocheted fabric, non-wovens, etc. for men, women and children: • coats, suits, ensembles, jackets, trousers, skirts, etc.</xkos:coreContentNote>
    <xkos:additionalContentNote xml:lang="en">- custom tailoring</xkos:additionalContentNote>
    <xkos:exclusionNote xml:lang="en">- manufacture of wearing apparel of furskins, see 18.30 - manufacture of wearing apparel of rubber or plastics not assembled by stitching but merely sealed together, see 25.13, 25.24 - repair of wearing apparel, see 52.74</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/18.2"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://ipsoeu.github.io/ramon-ld/nace/1990/18.23">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">18.23</dct:identifier>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">18.23</skos:notation>
    <skos:prefLabel xml:lang="en">Manufacture of underwear</skos:prefLabel>
    <skos:definition xml:lang="en">- manufacture of underwear made of woven, knitted or crocheted fabric, lace, etc. for men, women and children: • shirts, T-shirts, underpants, briefs, pyjamas, nightdresses, dressing gowns, blouses, slips, brassieres, corsets, etc.</skos:definition>
    <xkos:coreContentNote xml:lang="en">- manufacture of underwear made of woven, knitted or crocheted fabric, lace, etc. for men, women and children: • shirts, T-shirts, underpants, briefs, pyjamas, nightdresses, dressing gowns, blouses, slips, brassieres, corsets, etc.</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="en">- repair of wearing apparel, see 52.74</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/18.2"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://ipsoeu.github.io/ramon-ld/nace/1990/18.24">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990"/>
