    <skos:prefLabel xml:lang="en">Retreading and rebuilding of rubber tyres</skos:prefLabel>
    <skos:definition xml:lang="en">- tyre rebuilding and retreading</skos:definition>
    <xkos:coreContentNote xml:lang="en">- tyre rebuilding and retreading</xkos:coreContentNote>
    <xkos:exclusionNote xml:lang="en">- manufacture of rubber repair materials, see 25.13 - repair of punctured tyres, see 50.20</xkos:exclusionNote>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/25.1"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://ipsoeu.github.io/ramon-ld/nace/1990/25.13">
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990"/>
