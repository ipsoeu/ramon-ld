    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">01.4</skos:notation>
    <skos:prefLabel xml:lang="en">Agricultural and animal husbandry service activities, except veterinary activities</skos:prefLabel>
    <skos:broader rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/01"/>
    <skos:narrower rdf:resource="https://ipsoeu.github.io/ramon-ld/nace/1990/01.41"/>
